
<table id="main_table" cellpadding="0" cellspacing="10"><tbody><tr><td class="main">
		<p>Процентная ставка: <%= @params["percent"] %>%<br>
		Сумма кредита: <%= @params["credit"] %>
</p>

<table cellpadding="0" cellspacing="0" border="1" width="100%">
	<tbody>
		<tr type="header-main">
			<th type="header-main-base">номер месяца</th>
			<th type="header-main-base">погашение кредита</th>
			<th type="header-main-base">погашение %</th>
			<th type="header-main-base">общий взнос</th>
			<th type="header-main-base">остаток кредита</th>
		</tr>
		<% @table_result.each do |s| %>
		<tr>
			<td><%= s[:nom_moth] %></td>
			<td align="right"><%= s[:s_credit] %></td>
			<td align="right"><%= s[:s_percent] %></td>
			<td align="right"><%= s[:suma] %></td>
			<td align="right"><%= s[:ost_credit] %></td>
		</tr>	
		<% end %>
	</tbody>
</table>